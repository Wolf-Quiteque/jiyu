                                            <th scope="col">날짜</th>
                                            <th scope="col">OS</th>
                                            <th scope="col">활동</th>
                                            <th scope="col">IP</th>
                                            <th scope="col">상태</th>
                                        </tr>
                                    </thead>

                                    <!-- Table Body -->
                                    <tbody>
                                        <tr>
                                            <td>
                                                <div class="checkbox-button">
                                                    <label for="check_02">
                                                        <input type="checkbox" value="" id="check_02">
                                                        <i class="checkbox"></i>
                                                    </label>
                                                </div>
                                            </td>
                                            <td>1</td>
                                            <td>2020-04-23 00:00:00</td>
                                            <td>% 운영체제 %</td>
