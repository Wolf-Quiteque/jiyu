                                        <col width="5%">
                                        <col width="5%">
                                        <col width="18%">
                                        <col width="18%">
                                        <col width="18%">
                                        <col width="18%">
                                        <col width="18%">
                                    </colgroup>

                                    <!-- Table Header -->
                                    <thead>
                                        <tr>
                                            <th scope="col">
                                                <div class="checkbox-button">
                                                    <label for="check_01">
                                                        <input type="checkbox" value="" id="check_01">
                                                        <i class="checkbox"></i>
                                                    </label>
                                                </div>
