{{> ../../../views/templates/head }}

<body>
    {{> ../../../views/templates/guide/gnb }}

    <!-- Start of App Container -->
    <div class="app-container">

        {{> ../../../views/templates/guide/lnb }}

        <!-- Start of Main Container-->
        <main id="typography">
            <section class="content-wrapper">
                <!-- Content Wrapper Title -->
                <div class="wrapper-title">
                    <h2>Typography</h2>
                </div>

                <!-- Content Wrapper Body -->
                <div class="wrapper-body">


                    <div class="row">

                        <div class="col-lg-4 typings">
                            <h1 class="typings-title font-weight-normal">Heading | Normal</h1>
                            <div class="typings-body">
                                <h1 class="font-weight-normal">Heading 1</h1>
                                <h2 class="font-weight-normal">Heading 2</h2>
                                <h3 class="font-weight-normal">Heading 3</h3>
                                <p class="body-text-1 font-weight-normal">Body Text 1</p>
                                <p class="body-text-2 font-weight-normal">Body Text 2</p>
                                <p class="body-text-3 font-weight-normal">Body Text 3</p>
                                <small class="small-text-1 font-weight-normal">Small Text 1</small>
                                <small class="small-text-2 font-weight-normal">Small Text 2</small>
                            </div>
                        </div>

                        <div class="col-lg-4 typings">
                            <h1 class="typings-title font-weight-medium">Heading | Medium</h1>
                            <div class="typings-body">
                                <h1 class="font-weight-medium">Heading 1</h1>
                                <h2 class="font-weight-medium">Heading 2</h2>
                                <h3 class="font-weight-medium">Heading 3</h3>
                                <p class="body-text-1 font-weight-medium">Body Text 1</p>
                                <p class="body-text-2 font-weight-medium">Body Text 2</p>
                                <p class="body-text-3 font-weight-medium">Body Text 3</p>
                                <small class="small-text-1 font-weight-medium">Small Text 1</small>
                                <small class="small-text-2 font-weight-medium">Small Text 2</small>
                            </div>
                        </div>

                        <div class="col-lg-4 typings">
                            <h1 class="typings-title font-weight-bold">Heading | Bold</h1>
                            <div class="typings-body">
                                <h1 class="font-weight-bold">Heading 1</h1>
                                <h2 class="font-weight-bold">Heading 2</h2>
                                <h3 class="font-weight-bold">Heading 3</h3>
                                <p class="body-text-1 font-weight-bold">Body Text 1</p>
                                <p class="body-text-2 font-weight-bold">Body Text 2</p>
                                <p class="body-text-3 font-weight-bold">Body Text 3</p>
                                <small class="small-text-1 font-weight-bold">Small Text 1</small>
                                <small class="small-text-2 font-weight-bold">Small Text 2</small>
                            </div>
                        </div>

                    </div>

                </div>
            </section>

            <!-- Page Navigation -->
            <div class="page-navigation">
                <a href="#" class="page-previous">
                    <span>
                        <i class="iconify" data-icon="ion:arrow-back-sharp" data-inline="false"></i>
                        Colors
                    </span>
                </a>
                <a href="#" class="page-next">
                    <span>
                        Components
                        <i class="iconify" data-icon="ion:arrow-forward-sharp" data-inline="false"></i>
                    </span>
                </a>
            </div>
            <!-- End of Page Navigation -->
        </main>
        <!-- End of Main Container-->

    </div>
    <!-- End of App Container -->

</body>

</html>